# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  Matrix_CPP_config = debug
  Tests_config = debug
endif
ifeq ($(config),release)
  Matrix_CPP_config = release
  Tests_config = release
endif

PROJECTS := Matrix-CPP Tests

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

Matrix-CPP:
ifneq (,$(Matrix_CPP_config))
	@echo "==== Building Matrix-CPP ($(Matrix_CPP_config)) ===="
	@${MAKE} --no-print-directory -C . -f Matrix-CPP.make config=$(Matrix_CPP_config)
endif

Tests: Matrix-CPP
ifneq (,$(Tests_config))
	@echo "==== Building Tests ($(Tests_config)) ===="
	@${MAKE} --no-print-directory -C . -f Tests.make config=$(Tests_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f Matrix-CPP.make clean
	@${MAKE} --no-print-directory -C . -f Tests.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   Matrix-CPP"
	@echo "   Tests"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"